			<artifactId>gson</artifactId>
			<version>2.3</version>
		</dependency>


	</dependencies>
	<build>
		<finalName>cfm-web</finalName>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>
		</plugins>
	</build>



</project>
