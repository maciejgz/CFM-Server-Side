<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context.xsd">


	<context:annotation-config />

	<bean id="mainObject" class="pl.mg.cfm.springtest.MainObject">
		<property name="object" ref="component"></property>
		<property name="conObject" ref="constructorObject"></property>
	</bean>
	
	<bean id="component" class="pl.mg.cfm.springtest.ComponentObject">
		<property name="version" value="123"></property>
	</bean>

	<bean id="constructorObject" class="pl.mg.cfm.springtest.ConstructorObject">
		<constructor-arg name="version" value="1"></constructor-arg>
	</bean>


</beans>